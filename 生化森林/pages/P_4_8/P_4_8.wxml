<button bindtap='F' hover-class='F' disabled='{{q<1}}' plain='true'>B.鸟氨酸</button>
<button bindtap='F' hover-class='F' disabled='{{q<1}}' plain='true'>C.瓜氨酸</button>
<button bindtap='T' hover-class='F' disabled='{{q<1}}' plain='true'>D.天冬氨酸</button>
</view>
